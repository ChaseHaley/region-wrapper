{
  "name": "region-wrapper",
  "displayName": "Region Wrapper",
  "description": "A VS Code extension to easily wrap code blocks in regions, with a view to display them in the Activity Bar",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.wrapWithRegion"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.wrapWithRegion",
        "title": "Wrap with Region"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "regionTreeView",
          "name": "Regions"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swssr/region-wrapper"
  },
  "publisher": "swssr",
  "icon": "resources/icon.png",
  "bugs": {
    "url": "https://github.com/swssr/region-wrapper/issues"
  },
  "homepage": "https://github.com/swssr/region-wrapper/blob/main/README.md"
}